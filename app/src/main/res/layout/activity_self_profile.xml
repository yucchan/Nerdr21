<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SelfProfileActivity"
    android:background="@drawable/dwarf_warrior">

    <ImageView
        android:id="@+id/epLogoIMG"
        android:layout_width="wrap_content"
        android:layout_height="105dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="11dp"
        app:srcCompat="@drawable/nerdr_logo_large" />

    <ImageView
        android:id="@+id/spProfileImg"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_below="@id/epLogoIMG"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        app:srcCompat="@drawable/w1_profile"
        />


</RelativeLayout>